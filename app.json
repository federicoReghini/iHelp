{
  "expo": {
    "name": "ihelp",
    "slug": "ihelp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/senseibossicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/senseiold.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "it.fede.ihelp",
      "supportsTablet": true
    },
    "android": {
      "package": "it.fede.ihelp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
